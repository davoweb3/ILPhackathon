\n    const config = {\n      method: \"POST\",\n      headers:{ \n        'Content-Type': 'application/json',\n      },\n      body: JSON.stringify({\n        wallet: result\n      }), \n    }\n    let url = \"https://t77wj8-8081.csb.app/callalchemy\"\n    fetch(url, config)\n      .then(response => response.json())\n      .then((val)=>{\n        if(\"receipt\" in val){\n          setOpen(true)\n        }else{\n          setData(val.code)\n        }\n    }).catch((err)=>{\n      setData(err)\n      setOpen(false)\n    })\n  }\n\n\n  const handleErrorWebCam = (error) => {\n    console.log(error);\n  }\n  const handleScanWebCam = (result) => {\n    if (result) {\n      setData(`your fantom wallet is: ${result}`)\n      setToken(result)\n    }\n  }\n  const handleClaim = () =>{\n    if (token) { \n      if(!open){\n        setOpen(true)\n        postToToken(token)\n      }\n    }\n  }\n\n\n  return (\n    <div className=\"App\">\n      <div className=\"container\">\n        <label className='title'>Step 3</label>\n        <p>\n          Scan your registered wallet and show your QR address, Tokens will be sent to this address\n        </p>\n        <Grid container style={{ width: window.innerWidth < 1000 ? \"100%\" : \"25%\" }}>\n          <Grid item xl={12} lg={12} md={12} sm={12} xs={12} >\n            <QrReader\n              delay={300}\n              style={{ width: '100%' }}\n              onError={handleErrorWebCam}\n              onScan={handleScanWebCam}\n              facingMode=\"environment\"\n            />\n          </Grid>\n        </Grid>\n        <p>{data}</p>\n        <ButtonStyle Text={\"Claim a PET token!\"} onClick={handleClaim}></ButtonStyle>\n      </div>\n      <ModalInfo flag={open} close={handleClose}  ></ModalInfo>\n    </div>\n  );\n}\n\nexport default Token;\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\n//import App from './App';\nimport reportWebVitals from './reportWebVitals';\nimport Home from